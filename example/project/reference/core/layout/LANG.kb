LANG "en"
HEAD
	LOAD "_ROOT/project/shared/head"
	NAME "LANG statment | kytschBASIC"
HEAD CLOSE
BODY
	MAIN
		LOAD "_ROOT/project/shared/header"
		DIV "kb-row", "main-content"
			DIV "kb-col kb-col-24", "main-content-col"
				SWRITE
					PRINT "LANG","heading-command basic"
					LINE BREAK
					PRINT "LANGUAGE TYPE","heading-args"
				SWRITE CLOSE
				SWRITE
					PRINT "Starting the HTML page off and setting the language type. This should always be the first command executed."
				SWRITE CLOSE
				HEADING 3
					PRINT "Example"
				HEADING CLOSE
				CPRINT
LANG "en"
BODY
	SWRITE
		PRINT "Creating the HTML body."
	SWRITE CLOSE
BODY CLOSE
END
				CPRINT CLOSE
				HEADING 4
					PRINT "Output"
				HEADING CLOSE
				CPRINT
&lt;body&gt;
	&lt;p&gt;
		Creating the HTML body.
	&lt;/p&gt;
&lt;/body&gt;
				CPRINT CLOSE
				HEADING 4
					PRINT "See also"
