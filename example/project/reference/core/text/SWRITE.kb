LANG "en"
HEAD
	LOAD "$ROOT_FOLDER/project/shared/head"
	NAME "SWRITE statment | kytschBASIC"
HEAD CLOSE
BODY
	MAIN
		LOAD "$ROOT_FOLDER/project/shared/header"
		DIV "kb-row", "main-content"
			DIV "kb-col kb-col-24", "main-content-col"
				HEADING 2
					PRINT "SWRITE","basic"
				HEADING CLOSE
				SWRITE
					PRINT "Outputting to the screen as a paragraph."
				SWRITE CLOSE
				HEADING 3
					PRINT "Example"
				HEADING CLOSE
				CPRINT
SWRITE
	Hello World!
	I am a paragraph.
SWRITE CLOSE
				CPRINT CLOSE
				HEADING 4
					PRINT "Output"
				HEADING CLOSE
				CPRINT
Hello World!
I am a paragraph.
				CPRINT CLOSE
				HEADING 4
					PRINT "See also"
				HEADING CLOSE
				DIV "kb-col kb-col-24", "see-also"
					LINK "/reference/CPRINT"
						PRINT "CPRINT"
					LINK CLOSE
					PRINT "&comma;&nbsp;"
					LINK "/reference/LCASE"
						PRINT "LCASE"
					LINK CLOSE
					PRINT "&comma;&nbsp;"
					LINK "/reference/PRINT"
						PRINT "PRINT"
					LINK CLOSE
					PRINT "&comma;&nbsp;"
					LINK "/reference/UCASE"
						PRINT "UCASE"
					LINK CLOSE
				DIV CLOSE
			DIV CLOSE
		DIV CLOSE
	MAIN CLOSE
	LOAD "$ROOT_FOLDER/project/shared/footer"
BODY CLOSE
END
