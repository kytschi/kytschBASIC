								TROW CLOSE
								TROW
									TCELL
										WHITESPACE
									TCELL CLOSE
									TCELL
										BUTTON "btn_test";;; "button"
									TCELL CLOSE
									TCELL
										BUTTON "btn_test2"
											PRINT "button2"
										BUTTON CLOSE
									TCELL CLOSE
								TROW CLOSE
							TBODY CLOSE
						TABLE CLOSE
					SWRITE CLOSE
				DIV CLOSE
				HEADING 4
					PRINT "See also"
				HEADING CLOSE 4
				DIV "kb-col kb-col-24";"see-also"
					LINK "/reference/FORM"
						PRINT "FORM"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/CAPTCHA"
						PRINT "CAPTCHA"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/TEXTAREA"
						PRINT "TEXTAREA"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/NUMBERINPUT"
						PRINT "NUMBERINPUT"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/TEXTINPUT"
						PRINT "TEXTINPUT"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/BUTTON-SUBMIT"
						PRINT "BUTTON SUBMIT"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/_GET"
						PRINT "_GET"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/_POST"
						PRINT "_POST"
					LINK CLOSE
					PRINT " | "
					LINK "/reference/_REQUEST"
						PRINT "_REQUEST"
					LINK CLOSE
				DIV CLOSE
			DIV CLOSE
		DIV CLOSE
	LOAD "_ROOT/project/shared/footer"
	MAIN CLOSE
BODY CLOSE
