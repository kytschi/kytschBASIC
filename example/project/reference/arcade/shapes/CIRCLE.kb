	RGB 255,0,0,96
	CIRCLE 320,220,80

	RGB 255,0,0,97
	CIRCLE 320,220,60

	RGB 255,0,0,98
	CIRCLE 320,220,40

	RGB 255,0,0,98
	CIRCLE 320,220,20
BITMAP CLOSE
				CPRINT CLOSE
				HEADING 4
					PRINT "Output"
				HEADING CLOSE
				RGB 0,0,0
				BITMAP 0,0,640,480
					RGB 255,0,0,40
					CIRCLE 320,220,220

					RGB 255,0,0,60
