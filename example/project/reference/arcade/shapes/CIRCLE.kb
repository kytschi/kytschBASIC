LANG "en"
HEAD
	INCLUDE arcade
	LOAD "_ROOT/project/shared/head"
	NAME "CIRCLE statment | kytschBASIC"
HEAD CLOSE
BODY
	MAIN
		LOAD "_ROOT/project/shared/header"
		DIV "kb-row", "main-content"
			DIV "kb-col kb-col-24", "main-content-col"
				SWRITE
					PRINT "CIRCLE","heading-command basic"
					LINE BREAK
					PRINT "X, Y, RADIUS","heading-args"
					LINE BREAK
					PRINT "Requires arcade library","heading-required"
				SWRITE CLOSE
				SWRITE
					PRINT "The ability to draw a circle on a bitmap and display it on screen as an image."
				SWRITE CLOSE
				HEADING 3
					PRINT "Example"
				HEADING CLOSE
				CPRINT
SPRINT INCLUDE arcade
RGB 0,0,0
BITMAP 0,0,640,480
	RGB 255,0,0,40
	CIRCLE 320,220,220

	RGB 255,0,0,60
	CIRCLE 320,220,200

	RGB 255,0,0,70
	CIRCLE 320,220,180

	RGB 255,0,0,80
	CIRCLE 320,220,160

	RGB 255,0,0,90
