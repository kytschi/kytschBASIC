ANIMATION "updateOmegaEnginesFire", "false"
    GOTO updateOmegaShip("omega_shields", 70)
    GOTO updateOmegaShip("omega_engines", 0)
END ANIMATION
AFUNCTION "updateOmegaFire"
    userInputRemove("game-progress-41");
    if (!game_data.omega_engines) {
        user_continue.push(["game-progress-41", "game-progress-9"]);
        user_skip.push(["game-progress-29", "game-progress-41"]);
        animation_trigger.push(["game-progress-41", "updateOmegaEnginesFire", "false"]);
    } else {
        user_continue.push(["game-progress-41", "game-progress-27"]);
    }
END AFUNCTION
AFUNCTION "updateOmegaShip", "var_name", "var_value"    
    game_data[var_name] = var_value;
    WRITECOOKIE("kb_OMEGA", game_data);

    if (var_name == "omega_shields") {
        if (var_value == 0) {
            $("#omega-stats #omega-shields").html('<span class="red">' + var_value + '&percnt;&nbsp;[OFFLINE]</span>');
        } else if (var_value <= 39 && value > 0) {
            $("#omega-stats #omega-shields").html('<span class="red">' + var_value + '&percnt;&nbsp;[ONLINE]</span>');
        } else if (var_value <= 60 && value >= 40) {
            $("#omega-stats #omega-shields").html('<span class="yellow">' + var_value + '&percnt;&nbsp;[ONLINE]</span>');
        } else {
            $("#omega-stats #omega-shields").html('<span class="normal">' + var_value + '&percnt;&nbsp;[ONLINE]</span>');
        }
    }

    $("#omega-stats #omega-engines span").removeClass("normal").removeClass("red");
    if (var_name == "omega_engines" && var_value == 0) {
        $("#omega-stats #omega-engines span").addClass("red");
        $("#omega-stats #omega-engines span").html("offline");
    } else {
        $("#omega-stats #omega-engines span").addClass("red");
        $("#omega-stats #omega-engines span").html("online");
    }

    $("#omega-stats #omega-weapons span").removeClass("normal").removeClass("red");
    if (var_name == "omega_weapons" && var_value == 0) {
        $("#omega-stats #omega-weapons span").addClass("red");
        $("#omega-stats #omega-weapons span").html("offline");
    } else {
        $("#omega-stats #omega-weapons span").addClass("normal");
        $("#omega-stats #omega-weapons span").html("online");
    }

    updateOmegaFire(event);
END AFUNCTION